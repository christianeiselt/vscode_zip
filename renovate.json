  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "^extensions.ya?ml$"
      ],
      "matchStrings": [
        "datasource=github-releases"
      ]
    }
  ]
}
